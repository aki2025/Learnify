# MongoDB connection string
MONGO_URI=your-mongo-uri

# Secret key for Flask app (used for session management and security)
SECRET_KEY=your-secret-key

# GitHub Personal Access Token (for fetching repository content)
GITHUB_TOKEN=your-github-token

# OpenAI API Key (for AI-powered search and chatbot)
OPENAI_API_KEY=your-openai-api-key

# Secret key for JWT (used for user authentication tokens)
JWT_SECRET_KEY=your-jwt-secret-key

# Debug mode (set to True for development, False for production)
DEBUG=True

# Port for the Flask app (default is 5000)
PORT=5000